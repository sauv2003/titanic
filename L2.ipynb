   "source": [
    "from contextlib import contextmanager\n",
    "\n",
    "# Define a function that acts as a context manager\n",
    "@contextmanager\n",
    "def timer():\n",
    "    start = time.time()\n",
    "    print(\"Timer started...\")\n",
    "    try:\n",
    "        yield # Code inside the 'with' block runs here\n",
    "    finally:\n",
    "        end = time.time()\n",
    "        print(f\"Timer stopped. Duration: {end - start:.4f} seconds.\")\n",
    "\n",
    "# Usage\n",
    "with timer():\n",
    "    # Simulate some task\n",
    "    time.sleep(0.1)\n",
    "    print(\"Task executed.\")"
   ]
