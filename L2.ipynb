   "source": [
    "## Inheritance"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "4df41b29",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "This is a Silver Toyota vehicle.\n",
      "Beep! Beep!\n"
     ]
    }
   ],
   "source": [
    "# Parent Class\n",
    "class Vehicle:\n",
    "    def __init__(self, brand, color):\n",
    "        self.brand = brand\n",
    "        self.color = color\n",
    "\n",
    "    def describe(self):\n",
    "        print(f\"This is a {self.color} {self.brand} vehicle.\")\n",
    "\n",
    "# Child Class (inherits from Vehicle)\n",
    "class Car(Vehicle):\n",
    "    # The Car class automatically gets brand, color, and describe()\n",
    "    def __init__(self, brand, color, doors):\n",
    "        # 1. Call the parent's constructor using 'super()'\n",
    "        # This handles initializing the inherited attributes (brand, color)\n",
    "        super().__init__(brand, color)\n",
    "        # 2. Initialize the child's unique attribute\n",
    "        self.doors = doors\n",
    "\n",
    "    # Child's unique method\n",
    "    def honk(self):\n",
    "        print(\"Beep! Beep!\")\n",
    "\n",
    "# Create an instance of the child class\n",
    "my_car = Car(\"Toyota\", \"Silver\", 4)\n",
    "\n",
    "# Use inherited method\n",
    "my_car.describe() # Output: This is a Silver Toyota vehicle.\n",
    "\n",
    "# Use unique method\n",
    "my_car.honk()"
   ]
  },
