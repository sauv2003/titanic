    "## Advanced Techniques"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "d01177ae",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "--- STARTING FUNCTION ---\n",
      "Hello, Alex!\n",
      "--- FUNCTION FINISHED ---\n"
     ]
    }
   ],
   "source": [
    "# 1. The Decorator Function\n",
    "def announce_decorator(func):\n",
    "    # This inner function does the wrapping and modification\n",
    "    def wrapper(*args, **kwargs):\n",
    "        print(\"--- STARTING FUNCTION ---\")\n",
    "        # Call the original function\n",
    "        result = func(*args, **kwargs)\n",
    "        print(\"--- FUNCTION FINISHED ---\")\n",
    "        return result\n",
    "    return wrapper\n",
    "\n",
    "# 2. Applying the Decorator (Syntactic Sugar)\n",
    "@announce_decorator\n",
    "def greet(name):\n",
    "    print(f\"Hello, {name}!\")\n",
    "\n",
    "# When we call greet(), the wrapper function runs, adding the print lines.\n",
    "greet(\"Alex\")\n",
    "# Output:\n",
    "# --- STARTING FUNCTION ---\n",
    "# Hello, Alex!\n",
    "# --- FUNCTION FINISHED ---"
   ]
  },
